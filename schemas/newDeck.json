{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "creatorID": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 3
    },
    "description": {
      "type": "string"
    },
    "imgURL": {
      "type": "string"
    },
    "format": {
      "type": "string",
      "enum": [
        "standard",
        "future",
        "historic",
        "timeless",
        "gladiator",
        "pioneer",
        "explorer",
        "modern",
        "legacy",
        "pauper",
        "vintage",
        "penny",
        "commander",
        "oathbreaker",
        "standardbrawl",
        "brawl",
        "alchemy",
        "paupercommander",
        "oldschool",
        "premodern",
        "predh"
      ]
    }
  },
  "required": ["name", "format", "creatorID"]
}
